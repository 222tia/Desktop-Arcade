<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="500.0" prefWidth="650.0" spacing="20.0"
      style="-fx-background-color: #fffbf2; -fx-background-radius: 10px; -fx-border-color: #d67bf4; -fx-border-style: solid; -fx-border-width: 10px;"
      xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="edu.bsu.cs222.GUITTTMultiplayerChoice">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <Label text="Label"/>
   <Button fx:id="singleplayerTTT" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
           onAction="#onTTTSinglePlayerClick" prefHeight="25.0" prefWidth="150.0" style="-fx-background-color: #75fdfb;"
           text="Singleplayer" textFill="WHITE">
      <font>
         <Font name="Cooper Black" size="12.0"/>
      </font>
   </Button>
   <Button fx:id="multiplayerTTT" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
           onAction="#onTTTMultiPlayerClick" prefHeight="25.0" prefWidth="150.0" style="-fx-background-color: #75fdfb;"
           text="Multiplayer" textFill="WHITE">
      <font>
         <Font name="Cooper Black" size="12.0"/>
      </font>
   </Button>
</VBox>
